import java.io.*;
class operations
{
 public static void main(String[] args)
    {
  int a=6,b=15,c=8;
System.out.println(a==b);
System.out.println(a==c);
System.out.println(a>b);
System.out.println(a<b);
System.out.println(a>=c);
System.out.println(a<=b);
System.out.println(a!=b);
}
}

output:
D:\>javac operations.java

D:\>java operations
false
false
false
true
false
false
true
